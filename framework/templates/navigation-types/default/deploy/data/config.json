{
	"paths":
	{
		"cdn":"",
		"base":""
	},	
	"navigation":{
		"forceHashBang":false,
		"startBefore":true,
		"autoStart":true,
		"instantiateViews":true,
		"defaultView":"home"
	},
	"views":[
		{
			"id":"home",
			"class":"home-view",
			"route":"/",
			"content":"{base}data/home/content.json",
			"cache":true,
			"destroyable":true,
			"loadContent":true
		},
		{
			"id":"sub1",
			"class":"sub1-view",
			"content":"{base}data/home/sub1/content.json",
			"route":"/sub1",
			"destroyable":true,
			"loadContent":true,
			"cache":true
		}
	],
	"required":
	{
		"preloader":
		[
			{
				"id": "preloader-css",
				"src": "{base}css/preloader.css"
			}
		],
		"tracking":
		[
			{
				"id": "test-tracking",
				"src": "{base}data/tracking.json"
			}
		],
		"core":
		[
			{
				"id":"vendors",
				"src":"{base}js/vendors.js"
			}
		],
		"main":
		[
			{
				"content":"{base}data/main/content.json"
			},
			{
				"id": "main-css",
				"src": "{base}css/main.css"
			},
			{
				"id": "main-js",
				"src": "{base}js/main.js"
			}
		]
	}
}
