{
	"cacheContents":{
		"src":"{base}js/sw.js",
		"scope":"./"
	},	
	"paths":{
		"cdn":"",
		"base":"",
		"images":"{base}media/images/",
		"videos":"{base}media/videos/"
	},
	"navigation":{
		"forceHashBang":false,
		"startBefore":true,
		"autoStart":true,
		"instantiateViews":true,
		"defaultView":"home"
	},
	"conditions": {
        "mobile":{
            "browser":{
            	"mobile": true
            }
        },
        "desktop":{
            "browser":{
            	"desktop": true
            }
        },
        "mobile-size": {
            "browser":{
            	"mobile": true
            },
            "size": {
                "max-width": "767<"
            }
        },
        "ds":{
        	"size": {
        		"max-width": 768
        	}
        },
        "dm":{
        	"size": {
        		"max-width": 960
        	}
        },
        "dl":{
        	"size": {
        		"max-width": 1040
        	}
        },
        "full":{
        	"size": {
        		"max-width": 1600
    	    }
    	}
    },	
	"views":[
		{
			"id":"home",
			"class":"template-home-view",
			"route":"/",
			"content":[
				{
					"condition":"desktop",
					"file":"{base}data/home.json",
					"cache":true,
					"testestes":"teste"

				}
			],
			"destroyable":false,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub1",
			"class":"template-sub-view",
			"content":"{base}data/sub1.json",
			"route":"/sub-view-01",
			"destroyable":false,
			"loadContent":true,
			"cache":true
			
		},
		{
			"id":"sub2",
			"class":"template-sub-view",
			"content":"{base}data/sub2.json",
			"route":"/sub-view-02",
			"destroyable":false,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub3",
			"class":"template-sub-view",
			"content":"{base}data/sub3.json",
			"route":"/sub-view-03",
			"destroyable":false,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub4",
			"class":"template-sub-view",
			"content":"{base}data/sub4.json",
			"route":"/sub-view-04",
			"destroyable":false,
			"loadContent":true,
			"cache":true
		},
		{
			"id":"sub5",
			"class":"template-sub-view",
			"content":"{base}data/sub5.json",
			"route":"/sub-view-05",
			"destroyable":false,
			"loadContent":true,
			"cache":true
		}
	],
	"required":
	{
		"preloader":
		[
			{
				"id": "preloader-css",
				"src": "{base}css/preloader.css",
				"cache":true
			},
			{
				"id":"preloader-content",
				"content":"{base}data/preloader.json",
				"cache":true
			}
		],
		"core":
		[
			{
				"id":"vendors",
				"src":"{base}js/vendors.js",
				"cache":true
			}
		],
		"main":
		[
			{
				"id": "main-css",
				"src": "{base}css/main.css",
				"cache":true
			},
			{
				"id": "main-js",
				"src": [
					{
						"condition":"desktop",
						"file":"{base}js/main.js"
					}
				],
				"cache":true
			},
			{
				"content":[
					{
						"condition":"desktop",
						"file":"{base}data/main.json",
						"cache":true
					}
				],
				"cache":true
			}
		]
	}
}
