global-reset()

html, body
	margin: 0
	padding: 0

	noselect()

noselect
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;

.nav-container
	position: relative
	white-space: nowrap
	
.sub1-view, .sub2-view, .sub3-view, .home-view, .test-view
	display: fixed
